-- a.proto --
syntax = "proto2";

package pkg;

import "e.proto";

option (extendthatthing) = { foo:"no!" };

message Baz {
  optional Baz baz = 1;

  optional NestedBaz nested_baz = 2;

  optional EeNum fds = 3;

  message NestedBaz {
    optional string in_nested_baz = 1 [(extend_that_field) = "foo"];
  }

  extensions 4 to 5;
}

enum EeNum {
  option (extend_that_enum) = "unset";

  EE_X = 0;

  EE_Y = 1 [(extend_that_enum_value) = "okk"];

  EE_Z = 2;
